{
    "version":2,
    "builds":[
        {"src":"server.js","use":"@now/node"}
    ],
    "routes":[
        {
            "src":"/(.*)",
            "dest":"server.js"
        }
    ],
    "headers": [
    {
      "source": "/(.*)",
      "headers": [
        { "key": "Access-Control-Allow-Credentials", "value": "true" },
        { "key": "Access-Control-Allow-Origin", "value": "https://career-helper-frontend.vercel.app" },
        
      ]
    }
  ]
}
